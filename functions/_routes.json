{
  "version": 1,
  "include": ["/*"],
  "exclude": [],
  "routes": [
    {
      "pattern": "/",
      "function": "[[route]]"
    },
    {
      "pattern": "/other",
      "function": "[[route]]"
    },
    {
      "pattern": "/_next/*",
      "function": "[[route]]"
    },
    {
      "pattern": "/*",
      "function": "[[route]]"
    }
  ]
}
